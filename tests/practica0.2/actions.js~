var execFile = require('child_process').execFile;
var pty = require("pty.js");


module.exports = {
	
	"run":  function(userid,program,args)
	{
		return "runned program for "+userid;
	},
	"compiled":  function(userid)
	{
		return "compiled program for "+userid;
	},
	
	"compile": function(clientid,args,callbackError,callbackOK) {
        var compiler = "clang"
        var child    = execFile(compiler,args,function(err, stdout, stderr) 
        {
            if (err)
            callbackError(clientid,stderr);
            else
            callbackOK(clientid,stdout);
        })
    }



}
